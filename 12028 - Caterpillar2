#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i, j, L;
    scanf("%d",&L);
    int Li[L];

    for(i = 0; i < L; i++) {
        scanf("%d",&Li[i]);
    }
    printf("..╚⊙ ⊙╝..\n");
    for(i = 0; i < L; i++) {
        for(j = 0; j < Li[i]; j++) {
            printf(".");
        }
        printf("╚═(███)═╝\n");
    }
}
