#include <stdio.h>
int q[10] = {0};
int flag = 0;
void place(int row, int NQ);
int valid(int row, int col);
void display(void);

int main(void)
{
    int NQ;
    scanf("%d", &NQ);
	place(0, NQ);
	printf("%d", flag);
	return 0;
}

int valid(int row, int col)
{
    int i;
    for (i=0; i<=row-1; i++) {
        if (q[i] == col || q[i] == col-row+i || q[i] == col+row-i)
            return 0;
    }
    return 1;
}

