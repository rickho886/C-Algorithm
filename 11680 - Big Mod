#include <stdio.h>
long long bigmod ( long long a, long long p, long long m )
{
    long long res = 1;
    long long x = a;

    while ( p )
    {
        if ( p & 1 ) //p is odd
        {
            res = ( res * x ) % m;
        }
        x = ( x * x ) % m;
        p = p >> 1;
    }

    return res;
}

int main()
{
    int T;
    long long a, p, m;
    scanf("%d", &T);
    while(T--) {
        scanf("%lld%lld%lld", &a, &p, &m);
        printf("%lld\n",bigmod(a, p, m));
    }
}
